{
	"name": "peroks/guzzle-file-cache",
	"type": "library",
	"description": "A lightweight file cache for Guzzle 7+ implementing the PSR-16 Caching Interface",
	"minimum-stability": "stable",
	"version": "2.0.0",
	"license": "MIT",
	"authors": [
		{
			"name": "Per Egil Roksvaag",
			"homepage": "https://github.com/peroks"
		}
	],
	"autoload": {
		"psr-4": {
			"Peroks\\GuzzleFileCache\\": "src/"
		}
	},
	"require": {
		"php": "^8.1",
		"psr/simple-cache": "^3.0",
		"guzzlehttp/promises": "^2.0",
		"guzzlehttp/psr7": "^2.5"
	},
	"config": {
		"preferred-install": "dist",
		"sort-packages": true
	}
}
